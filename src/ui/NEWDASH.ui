}

QRadioButton::indicator {
    width: 20px;
    height: 20px;
    background: #1a1a1a;
    border: 2px solid #2a2a2a;
    border-radius: 10px;
}

QRadioButton::indicator:checked {
    background: #00d4ff;
    border-color: #00d4ff;
}</string>
